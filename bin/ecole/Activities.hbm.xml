<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="ecole.Activities" table="ACTIVITIES">
        <composite-id name="compId" class="ecole.Activities">
        <key-property name="Niveau" column="NIVEAU" type="int"/>
        <key-property name="Nom" column="NOM" />
        </composite-id>
  

        <property name="equipe"  column ="EQUIPE"/>

    </class>


</hibernate-mapping>
