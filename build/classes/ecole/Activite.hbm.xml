<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ecole.pojo.Activite" table="activites">
        <composite-id name="id" class="ecole.pojo.ActiviteId">
        <key-property name="niveau" column="NIVEAU" type="int"/>
        <key-property name="nom" column="NOM" type="string"/>
        </composite-id>      
        
        <property name="equipe"  column ="EQUIPE"/>

    </class>


</hibernate-mapping>
