<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ecole.pojo.Resultat" table="resultats">
    <composite-id name="compId" class="ecole.pojo.Resultat">
        <key-property name="id_eleve"  type="int" column="NUM_ELEVE" />
        <key-property name="id_cours"  type="int"  column="NUM_COURS"/>
    </composite-id>
	

    <property name ="points" type="int" column="POINTS" />
</class>

</hibernate-mapping>
